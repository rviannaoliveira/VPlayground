<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.rviannaoliveira.vphysicsbasedanimation">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ChainSpringActivity"/>
        <activity android:name=".DragBackSpringActivity"/>
        <activity android:name=".FlingActivity"/>
        <activity android:name=".RotateSpringActivity"/>
        <activity android:name=".SimpleSpringActivity"/>
        <activity android:name=".CleanSpringActivity" />
    </application>

</manifest>